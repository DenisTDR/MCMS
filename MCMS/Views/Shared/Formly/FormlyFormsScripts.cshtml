@using MCMS.Helpers
@if (!Env.GetBool("FORMLY_DEBUG"))
{
    var basePath = Env.Get("FORMLY_SCRIPTS_PATH") is {} path && !string.IsNullOrEmpty(path) ? path : "~";

    <script src="@basePath/formly-form/runtime-es2015.js" type="module" asp-append-version="true"></script>
    <script src="@basePath/formly-form/runtime-es5.js" nomodule defer asp-append-version="true"></script>
    <script src="@basePath/formly-form/polyfills-es5.js" nomodule defer asp-append-version="true"></script>
    <script src="@basePath/formly-form/polyfills-es2015.js" type="module" asp-append-version="true"></script>
    <script src="@basePath/formly-form/styles-es2015.js" type="module" asp-append-version="true"></script>
    <script src="@basePath/formly-form/styles-es5.js" nomodule defer asp-append-version="true"></script>
@*     <script src="@basePath/formly-form/vendor-es2015.js" type="module" asp-append-version="true"></script> *@
@*     <script src="@basePath/formly-form/vendor-es5.js" nomodule defer asp-append-version="true"></script> *@
    <script src="@basePath/formly-form/main-es2015.js" type="module" asp-append-version="true"></script>
    <script src="@basePath/formly-form/main-es5.js" nomodule defer asp-append-version="true"></script>
}