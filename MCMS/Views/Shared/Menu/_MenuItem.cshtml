@using MCMS.Display.Menu
@model MCMS.Display.Menu.IMenuItem

@if (!Model.SatisfiedByUSer(User))
{
    return;
}

@if (Model is MenuLink link)
{
    @await Html.PartialAsync("Menu/_MenuLink", link)
}
else if (Model is MenuSection section)
{
    @await Html.PartialAsync("Menu/_MenuSection", section)
}